ion: line-through;\n  color: #999;\n  margin-right: 0.5rem;\n}\n\n.offer-note {\n  font-size: 0.95rem;\n  font-weight: 600;\n  color: #333;\n}\n\n.countdown {\n  font-family: monospace;\n  font-size: 1.55rem;\n  margin-top: 1rem;\n}\n.countdown-grid {\n  display: flex;\n  justify-content: center;\n  gap: 1.25rem;\n  margin-top: 0.5rem;\n}\n\n.countdown-block {\n  color: #777;\n  text-align: center;\n  font-family: monospace;\n}\n\n.countdown-block div:first-child {\n  font-size: 2rem;\n  color: #333;\n}\n\n.countdown-label {\n  font-size: 0.75rem;\n  color: #777;\n  margin-top: 0.25rem;\n  text-transform: uppercase;\n  letter-spacing: 0.5px;\n}\n\n.promo-banner-img-wrapper {\n  margin-bottom: 1.5rem;\n  text-align: center;\n}\n\n.promo-banner-img {\n  width: 100%;\n  max-width: 100%;\n  border-radius: var(--border-radius);\n  box-shadow: 0 4px 8px rgba(0,0,0,0.1);\n  height: auto;\n}\n\n\n.product-image-wrapper {\n  position: absolute;\n  top: -0.1rem;\n  right: 1.5rem;\n  width: 60px;\n  height: 60px;\n}\n\n.product-image-wrapper1{\n  position: absolute;\n  top: 1.1rem;\n  right: 1.5rem;\n  width: 60px;\n  height: 60px;\n}\n\n.bundle-product-image {\n  width: 100%;\n  height: 100%;\n\n}\n\n.gift-icon {\n  width: 18px;\n  height: 18px;\n  object-fit: contain;\n}\n\n.gift-icon2 {\n  width: 20px;\n  height: 20px;\n  object-fit: contain;\n}\n\n.gift-icon3 {\n  width: 22px;\n  height: 22px;\n  object-fit: contain;\n}\n\n@media screen and (min-width: 601px) {\n  .promo-banner-img {\n    width: 35% !important; /* moitié de la taille sur PC */\n  }\n}\n\n@media screen and (max-width: 600px) {\n  .promo-banner-img {\n    width: 80.66% !important; /* 2/3 de la taille sur mobile */\n  }\n}\n.promo-banner-img {\n  width: auto;\n  height: auto;\n  max-width: none;\n  display: block;\n  margin: 0 auto;\n}\n.tp-rating {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n}\n\n.tp-stars-img {\n  height: 18px;\n  width: auto;\n  display: inline-block;\n}\n\n.rating-text {\n  font-size: 0.875rem;\n  color: #666;\n}\n\n.countdown-enhanced {\n  font-family: monospace;\n  font-size: 2rem;\n  font-weight: bold;\n  letter-spacing: 2px;\n  margin-top: 0.5rem;\n  color: #333;\n}\n\n.countdown-labels {\n  display: grid;\n  grid-template-columns: repeat(4, auto);\n  justify-content: center;\n  gap: 1.6rem;\n  font-size: 0.7rem;\n  text-transform: uppercase;\n  color: #999;\n  letter-spacing: 1px;\n  margin-top: 0.25rem;\n}\n\n.countdown-labels > div {\n  text-align: center;\n}\n\n.award-text {\n  text-align: center;\n  font-size: 0.85rem;\n  font-weight: 700;\n  margin: 0.4rem 0 0.6rem;\n  color: #333;\n}\n\n</style>\n\n<script>\n(function() {\n  const form = document.getElementById('bundle-form');\n  if (!form) return;\n\n  form.addEventListener('submit', function(e) {\n    e.preventDefault();\n\n    // Vérifie si un bouton \"Acheter maintenant\" existe vraiment\n    const selectedRadio = form.querySelector('input[name=\"bundle\"]:checked');\n    if (!selectedRadio) {\n      alert(\"Veuillez sélectionner une offre avant de continuer.\");\n      return;\n    }\n\n    // Récupérer l'ID du variant et la quantité choisie\n    const variantId = parseInt(selectedRadio.dataset.variantId, 10);\n    const qty = parseInt(selectedRadio.value, 10);\n\n    const offerPrices = {\n      1: { old: 79.90, new: 39.90 },\n      2: { old: 109.90, new: 54.90 },\n      3: { old: 199.90, new: 64.90 }\n    };\n\n    const properties = {\n      'Offre': `${qty}-Mois`,\n      'Prix original': `${offerPrices[qty].old.toFixed(2).replace('.', ',')}€`,\n      'Prix réduit': `${offerPrices[qty].new.toFixed(2).replace('.', ',')}€`,\n      'Économie': `${(offerPrices[qty].old - offerPrices[qty].new).toFixed(2).replace('.', ',')}€`\n    };\n\n    if (qty === 1) properties['Cadeau'] = '15 JOURS OFFERT';\n    else if (qty === 2) properties['Cadeau'] = '30 JOURS OFFERT';\n    else if (qty === 3) properties['Cadeau'] = '45 JOURS OFFERT';\n\n    const items = [{\n      id: variantId,\n      quantity: 1,\n      properties\n    }];\n\n    fetch('/cart/add.js', {\n      method: 'POST',\n      headers: { 'Content-Type': 'application/json' },\n      body: JSON.stringify({ items })\n    })\n    .then(response => {\n      if (!response.ok) throw new Error(\"Erreur lors de l'ajout au panier\");\n      return response.json();\n    })\n    .then(() => window.location.href = '/checkout')\n    .catch(error => {\n      console.error(error);\n      alert(\"Erreur lors de l’ajout au panier, veuillez réessayer.\");\n    });\n  });\n})();\n\n\nfunction startCountdown(durationSeconds) {\n  let timer = durationSeconds;\n  const daysSpan = document.getElementById(\"days\");\n  const hoursSpan = document.getElementById(\"hours\");\n  const minutesSpan = document.getElementById(\"minutes\");\n  const secondsSpan = document.getElementById(\"seconds\");\n\n  const interval = setInterval(() => {\n    const days = Math.floor(timer / (60 * 60 * 24));\n    const hours = Math.floor((timer % (60 * 60 * 24)) / 3600);\n    const minutes = Math.floor((timer % 3600) / 60);\n    const seconds = timer % 60;\n\n    daysSpan.textContent = String(days).padStart(2, \"0\");\n    hoursSpan.textContent = String(hours).padStart(2, \"0\");\n    minutesSpan.textContent = String(minutes).padStart(2, \"0\");\n    secondsSpan.textContent = String(seconds).padStart(2, \"0\");\n\n    if (timer-- <= 0) clearInterval(interval);\n  }, 1000);\n}\n\ndocument.addEventListener(\"DOMContentLoaded\", () => {\n  startCountdown(995); // 15 minutes\n});\n\n\ndocument.addEventListener(\"DOMContentLoaded\", () => {\n  // Change le stock après 25 secondes\n  setTimeout(() => {\n    const stockDiv = document.querySelector(\".stock\");\n    if (stockDiv) {\n      stockDiv.innerHTML = \"● Stock restant : 5\";\n    }\n  }, 25000);\n});\n\n</script>",
