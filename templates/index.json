        "liquid_jNbhfY": {
          "type": "liquid",
          "settings": {
            "liquid": "<div class=\"product-page\">\n  <!-- Rating -->\n  <ul class=\"rating\" aria-label=\"Note moyenne : 4,8 sur 5 étoiles\">\n    <li><svg class=\"star\" viewBox=\"0 0 20 20\"><defs><linearGradient id=\"half\"><stop offset=\"50%\" stop-color=\"var(--star-fill)\"/><stop offset=\"50%\" stop-color=\"var(--star-empty)\"/></linearGradient></defs><path fill=\"var(--star-fill)\" d=\"M10 15l-5.878 3.09 1.122-6.545L.489 6.91l6.562-.955L10 0l2.949 5.955 6.562.955-4.755 4.635 1.122 6.545z\"/></svg></li>\n    <li><svg class=\"star\" viewBox=\"0 0 20 20\"><path fill=\"var(--star-fill)\" d=\"M10 15l-5.878 3.09 1.122-6.545L.489 6.91l6.562-.955L10 0l2.949 5.955 6.562.955-4.755 4.635 1.122 6.545z\"/></svg></li>\n    <li><svg class=\"star\" viewBox=\"0 0 20 20\"><path fill=\"var(--star-fill)\" d=\"M10 15l-5.878 3.09 1.122-6.545L.489 6.91l6.562-.955L10 0l2.949 5.955 6.562.955-4.755 4.635 1.122 6.545z\"/></svg></li>\n    <li><svg class=\"star\" viewBox=\"0 0 20 20\"><path fill=\"var(--star-fill)\" d=\"M10 15l-5.878 3.09 1.122-6.545L.489 6.91l6.562-.955L10 0l2.949 5.955 6.562.955-4.755 4.635 1.122 6.545z\"/></svg></li>\n    <li><svg class=\"star half\" viewBox=\"0 0 20 20\"><path fill=\"url(#half)\" d=\"M10 15l-5.878 3.09 1.122-6.545L.489 6.91l6.562-.955L10 0v15z\"/></svg></li>\n    <li class=\"rating-text\">4,8 (<span>12</span> avis)</li>\n  </ul>\n\n  <!-- Title + Price -->\n  <h1 class=\"product-title\">Lueur de Perle — Bougie Perlée</h1>\n  <div class=\"price\" aria-label=\"Prix réduit de 59,90 € à 35,90 €\">\n    <span class=\"new\">€35,90</span>\n    <span class=\"old\">€59,90</span>\n  </div>\n\n  <!-- Stock -->\n  <div class=\"stock\" role=\"status\" aria-live=\"polite\">En stock : <strong>12 restants</strong></div>\n\n  <!-- Promo banner -->\n  <div class=\"promo-banner\" role=\"region\" aria-label=\"Promotion fête des mères, 50% de réduction, stock limité\">\n    <p class=\"promo-label\">PROMO FÊTE DES MÈRES</p>\n    <p class=\"promo-big\">-30%</p>\n    <p class=\"promo-small\">Stock limité</p>\n  </div>\n\n  <!-- Bundle options -->\n  <form class=\"bundle-offer\" aria-labelledby=\"bundle-title\">\n    <h2 id=\"bundle-title\" class=\"sr-only\">Choisissez votre offre</h2>\n\n    <input type=\"radio\" id=\"offer1\" name=\"bundle\" value=\"1\" checked hidden>\n    <label for=\"offer1\" class=\"bundle-option\" tabindex=\"0\">\n      <div class=\"bundle-left\">\n        <span class=\"bundle-radio\" aria-hidden=\"true\"></span>\n        <div><p class=\"bundle-title\">Lueur de Perle — Bougie Perlée</p></div>\n      </div>\n      <div class=\"bundle-price\">\n        <span class=\"bundle-price-new\">€35,90</span>\n        <span class=\"bundle-price-old\">€59,90</span>\n      </div>\n    </label>\n\n    <input type=\"radio\" id=\"offer2\" name=\"bundle\" value=\"2\" hidden>\n    <label for=\"offer2\" class=\"bundle-option\" tabindex=\"0\">\n      <div class=\"bundle-left\">\n        <span class=\"bundle-radio\" aria-hidden=\"true\"></span>\n        <div><p class=\"bundle-title\">Achetez-en 2 et boîte de mèches offerte</p></div>\n      </div>\n      <div class=\"bundle-price\">\n        <span class=\"bundle-price-new\">€59,90</span>\n        <span class=\"bundle-price-old\">€75,90</span>\n      </div>\n      <span class=\"best-offer\" aria-hidden=\"true\">Meilleure offre !</span>\n    </label>\n\n    <input type=\"radio\" id=\"offer3\" name=\"bundle\" value=\"3\" hidden>\n    <label for=\"offer3\" class=\"bundle-option\" tabindex=\"0\">\n      <div class=\"bundle-left\">\n        <span class=\"bundle-radio\" aria-hidden=\"true\"></span>\n        <div><p class=\"bundle-title\">Achetez-en 3 et 2 mèches offertes</p></div>\n      </div>\n      <div class=\"bundle-price\">\n        <span class=\"bundle-price-new\">€79,90</span>\n        <span class=\"bundle-price-old\">€105,90</span>\n      </div>\n    </label>\n  </form>\n\n  <!-- Color selection -->\n  <div id=\"color-selection\" class=\"color-section\" style=\"display:none; margin-top: 1.5rem;\">\n    <h3 style=\"margin-bottom: 0.5rem;\">Choisissez les couleurs :</h3>\n    <div id=\"color-lines\"></div>\n    <p class=\"error-message\" style=\"color: red; display: none; font-size: 0.9rem;\">\n      Veuillez sélectionner une couleur pour chaque bougie.\n    </p>\n  </div>\n\n  <!-- Extra box add-on -->\n  <div id=\"extra-box\" style=\"display:none; margin-top: 1.5rem;\">\n    <label for=\"add-wick-box\" class=\"wick-box-label\">\n      <input type=\"checkbox\" id=\"add-wick-box\">\n      <span class=\"wick-box-custom\"></span>\n      <span class=\"wick-box-text\">Ajouter une boîte à mèches (+€5,90)</span>\n    </label>\n  </div>\n</div>\n\n<style>\n  :root {\n    --border-radius: 3vw;\n    --primary: #9C6644;\n    --accent: #EDE0D4;\n    --star-fill: #EDE0D4;\n    --star-empty: #E6CCB2;\n    --bg-banner: linear-gradient(135deg, #EDE0D4, #9C6644);\n  }\n\n  /* ... ton CSS existant ... */\n\n  /* Color-selection enhancements */\n  .color-line {\n    display: flex;\n    gap: 1rem;\n    margin-bottom: 1rem;\n    align-items: center;\n  }\n  .color-line::before {\n    content: attr(data-label);\n    width: 80px;\n    font-weight: 600;\n    color: #333;\n  }\n  .color-option {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    gap: 0.25rem;\n  }\n  .color-option .circle {\n    width: 40px;\n    height: 40px;\n    border-radius: 50%;\n    border: 2px solid transparent;\n    cursor: pointer;\n    background-size: cover;\n    background-position: center;\n  }\n  .color-option.selected .circle {\n    border-color: var(--primary);\n  }\n  .color-name {\n    font-size: 0.75rem;\n    text-transform: capitalize;\n    color: #555;\n  }\n\n  /* Custom checkbox for wick box */\n  .wick-box-label {\n    display: inline-flex;\n    align-items: center;\n    cursor: pointer;\n    gap: 0.5rem;\n    user-select: none;\n  }\n  .wick-box-label input {\n    display: none;\n  }\n  .wick-box-custom {\n    width: 20px;\n    height: 20px;\n    border: 2px solid #ccc;\n    border-radius: 4px;\n    position: relative;\n    transition: border-color 0.3s, background-color 0.3s;\n  }\n  .wick-box-custom::after {\n    content: '';\n    position: absolute;\n    top: 2px; left: 6px;\n    width: 6px; height: 12px;\n    border-right: 2px solid white;\n    border-bottom: 2px solid white;\n    transform: rotate(45deg);\n    opacity: 0;\n    transition: opacity 0.2s;\n  }\n  .wick-box-label input:checked + .wick-box-custom {\n    background-color: var(--primary);\n    border-color: var(--primary);\n  }\n  .wick-box-label input:checked + .wick-box-custom::after {\n    opacity: 1;\n  }\n\n  .wick-box-text {\n    font-size: 0.9rem;\n    color: #333;\n  }\n</style>\n\n<script>\n  const bundleRadios        = document.querySelectorAll('input[name=\"bundle\"]');\n  const colorSection        = document.getElementById('color-selection');\n  const colorLinesContainer = document.getElementById('color-lines');\n  const extraBox            = document.getElementById('extra-box');\n  const errorMsg            = document.querySelector('.error-message');\n\n  const colors = [\n    { name: 'blanc', class: 'color-blanc' },\n    { name: 'marron', class: 'color-marron' },\n    { name: 'noir', class: 'color-noir' }\n  ];\n\n  function createColorLine(index) {\n    const line = document.createElement('div');\n    line.className = 'color-line';\n    line.dataset.label = `Bougie ${index + 1}`; // Ajout du label\n\n    colors.forEach(color => {\n      const wrapper = document.createElement('div');\n      wrapper.className = 'color-option';\n\n      const circle = document.createElement('div');\n      circle.className = `circle ${color.class}`;\n      circle.dataset.line = index;\n      circle.dataset.color = color.name;\n      circle.addEventListener('click', () => {\n        document.querySelectorAll(`.circle[data-line='${index}']`)\n                .forEach(el => el.parentNode.classList.remove('selected'));\n        wrapper.classList.add('selected');\n      });\n\n      const name = document.createElement('div');\n      name.className = 'color-name';\n      name.textContent = color.name;\n\n      wrapper.append(circle, name);\n      line.appendChild(wrapper);\n    });\n\n    return line;\n  }\n\n  function updateColorSelection(qty) {\n    colorLinesContainer.innerHTML = '';\n    for (let i = 0; i < qty; i++) {\n      colorLinesContainer.appendChild(createColorLine(i));\n    }\n  }\n\n  function showOptions(qty) {\n    colorSection.style.display = 'block';\n    extraBox.style.display     = 'block';\n    updateColorSelection(qty);\n  }\n\n  function validateColors(qty) {\n    for (let i = 0; i < qty; i++) {\n      if (!document.querySelector(`.color-line[data-label=\"Bougie ${i + 1}\"] .selected`)) {\n        return false;\n      }\n    }\n    return true;\n  }\n\n  bundleRadios.forEach(radio => {\n    radio.addEventListener('change', () => {\n      showOptions(parseInt(radio.value, 10));\n      errorMsg.style.display = 'none';\n    });\n  });\n\n  document.querySelector('.bundle-offer').addEventListener('submit', e => {\n    const qty = parseInt(document.querySelector('input[name=\"bundle\"]:checked').value, 10);\n    if (!validateColors(qty)) {\n      e.preventDefault();\n      errorMsg.style.display = 'block';\n    } else {\n      errorMsg.style.display = 'none';\n    }\n  });\n\n  // Affichage initial\n  document.addEventListener('DOMContentLoaded', () => {\n    const initialRadio = document.querySelector('input[name=\"bundle\"]:checked');\n    if (initialRadio) {\n      showOptions(parseInt(initialRadio.value, 10));\n    }\n  });\n</script>"
